z + y = 0
z + (y + x) = 0
z + ((y + 12) + x) = 0
z + (((y + w) + 12) + x) = 0
z + (((0 + w) + 12) + x) = 0
z + ((w + 12) + x) = 0
(z * y) + ((w + 12) + x) = 0
(z * (y + 1)) + ((w + 12) + x) = 0
(z * ((y * x) + 1)) + ((w + 12) + x) = 0
(z * (((y + 25) * x) + 1)) + ((w + 12) + x) = 0
(z * (((0 + 25) * x) + 1)) + ((w + 12) + x) = 0
(z * (25 * x) + 1) + ((w + 12) + x) = 0
(z * (25 * (1 - x)) + 1) + ((w + 12) + (1 - x)) = 0
(z * (25 * (1 - exponent(0, abs(x - w)) )) + 1) + ((w + 12) + (1 - exponent(0, abs(x - w)) )) = 0
(z * (25 * (1 - exponent(0, abs((x - 12) - w)) )) + 1) + ((w + 12) + (1 - exponent(0, abs((x - 12) - w)) )) = 0
